dosseg
.MODEL SMALL 
.DATA 
NUM DW 8 
RESULT DW (?) 	 
.CODE 
MAIN PROC 
MOV AX, @DATA	 
MOV DS, AX 
MOV AX, 01 
MOV CX, NUM 
CMP CX, 00 		
JE LOOP1 			
MOV BX, CX 
CALL FACT 	 
LOOP1 
: MOV RESULT, AX		 
MOV AH, 4CH 
INT 21H 
MAIN ENDP		
FACT PROC 
CMP BX, 01 
JZ LOOP2 
PUSH BX 
DEC BX 
CALL FACT 		
PROCEDURE POP BX 
MUL BX 
RET	
LOOP2 
: MOV AX, 01 
RET	
FACT ENDP	
END
